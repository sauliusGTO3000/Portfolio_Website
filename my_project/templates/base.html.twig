<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-125416630-1"></script>
            <script>
                window.dataLayer = window.dataLayer || [];
                function gtag(){dataLayer.push(arguments);}
                gtag('js', new Date());

                gtag('config', 'UA-125416630-1');
            </script>
        <link rel="icon" href="https://sauliusgto3000.lt//styleelements/favicon.png">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset("build/css/app.css") }}">
            <link rel="stylesheet" type="text/css" href="{{ asset("build/css/homepage.css") }}">
            <link rel="stylesheet" type="text/css" href="{{ asset("build/css/about.css") }}">
            <link rel="stylesheet" type="text/css" href="{{ asset("build/css/projects.css") }}">
            <link rel="stylesheet" type="text/css" href="{{ asset("build/css/contacts.css") }}">
        {% endblock %}
    </head>
    <body>
        {% block navbar %}
        {% endblock %}
        {% block homepage %}
        {% endblock %}
        {% block body %}{% endblock %}
        {% block about %}
            {% include 'about/about.html.twig' %}
        {% endblock %}
        {% block projects %}
            {% include 'projects/projects.html.twig' %}
        {% endblock %}
        {% block contacts %}
            {% include 'contacts/contacts.html.twig' %}
        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('build/js/app.js') }}"></script>
        {% endblock %}
    </body>
</html>
